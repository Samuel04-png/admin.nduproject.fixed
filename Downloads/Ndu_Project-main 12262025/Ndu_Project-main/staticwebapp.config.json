{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",
      "/*.js",
      "/*.css",
      "/*.png",
      "/*.jpg",
      "/*.svg",
      "/*.webp",
      "/*.wasm",
      "/*.ico"
    ]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  },
  "routes": [
    {
      "route": "/index.html",
      "headers": {
        "cache-control": "no-cache, no-store, must-revalidate"
      }
    },
    {
      "route": "/",
      "headers": {
        "cache-control": "no-cache, no-store, must-revalidate"
      }
    },
    {
      "route": "/assets/*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/flutter.js",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    }
  ],
  "mimeTypes": {
    ".wasm": "application/wasm"
  }
}
